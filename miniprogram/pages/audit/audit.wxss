/* miniprogram/pages/audit/audit.wxss */
@keyframes ani-demo-spin {
    from { transform: rotate(0deg);}
    50%  { transform: rotate(180deg);}
    to   { transform: rotate(360deg);}
}
.icon-load{
    animation: ani-demo-spin 1s linear infinite;
}

.cell-panel-demo{
    display: block;
    margin-top: 15px;
}

.PASS{
  color: #19be6b
}

.REJECTED{
  color: #ed3f14
}